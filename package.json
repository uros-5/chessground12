    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "chai": "^4.2.0",
    "eslint": "^7",
    "esm": "^3.2.25",
    "jsdom": "^17.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.4.0",
    "prettier": "^2",
    "rollup": "^2",
