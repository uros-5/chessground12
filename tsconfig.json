{
  "include": [
    "src/*.ts"
  ],
  "exclude": [],
  "compilerOptions": {
    "noEmitOnError": false,
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "noImplicitReturns": true,
    "module": "esnext",
    "target": "ES2017",
    "lib": [
      "DOM",
      "ES2017"
    ]
  }
}